<div class="container well well-sm" style="width: 500px; margin: 0 auto; margin-top: 20px">
    <form action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST"
          class="fos_user_resetting_reset">
        {% form_theme form 'bootstrap_3_layout.html.twig' %}
        {{ form_widget(form) }}
        <div>
            <input type="submit" value="{{ 'resetting.reset.submit'|trans({}, 'FOSUserBundle') }}"/>
        </div>
    </form>
</div>
